{
  "expo": {
    "name": "chat-app",
    "description": "This project is really great.",
    "slug": "chat-app",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/famicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.nitesh.demo",
      "versionCode": 1,
      "permissions": ["CAMERA_ROLL"],
      "icon": "./assets/famicon.png"
    }
  }
}